  User devops
  LogLevel ERROR
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host *.moar.lan
  User devops
  LogLevel ERROR
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host 192.168.121.*
  User devops
  LogLevel ERROR
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host 10.10.17.
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host 10.10.11.
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host 10.10.*
  User devops
  LogLevel ERROR

Host carbon
  ForwardX11 yes
  ForwardX11Trusted yes
Host moar
  ForwardX11 yes
  ForwardX11Trusted yes
Host nebula
  ForwardX11 yes
  ForwardX11Trusted yes

Host *.zt
  Compression yes
  ForwardX11 yes

Host *.ts
  Compression yes
  ForwardX11 yes

Host github.com
    IdentityAgent /var/run/user/1000/yubikey-agent.sock
    IdentitiesOnly yes
