# vi: set ft=sshconfig
Host *.lan
  User devops
  LogLevel ERROR
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  SetEnv P10K_CUSTOM=yes
Host 192.168.1.*
  User devops
  LogLevel ERROR
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host *.moar.lan
  User devops
  LogLevel ERROR
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host 192.168.121.*
  User devops
  LogLevel ERROR
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host 10.10.17.*
  ProxyCommand /usr/bin/netcat -x 172.23.0.2:8443 %h %p
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host 10.10.11.*
  ProxyCommand /usr/bin/netcat -x 172.23.0.2:8443 %h %p
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
Host 10.10.*
  IdentityFile ~/.ssh/id_ed25519
  User devops
  LogLevel ERROR

# https://github.com/neovim/neovim/issues/7054
# ForwardX11Trusted yes

Host carbon
  IdentityFile ~/.ssh/id_ed25519
  ForwardX11 yes
  ForwardX11Trusted yes
Host moar
  IdentityFile ~/.ssh/id_ed25519
  ForwardX11 yes
  ForwardX11Trusted yes
Host nebula
  IdentityFile ~/.ssh/id_ed25519
  ForwardX11 yes
  ForwardX11Trusted yes

Host *.zt
  IdentityFile ~/.ssh/id_ed25519
  Compression yes
  ForwardX11 yes

Host *.ts
  IdentityFile ~/.ssh/id_ed25519
  Compression yes
  ForwardX11 yes

Host github.com
    IdentityAgent /var/run/user/1000/yubikey-agent.sock
    IdentitiesOnly yes
