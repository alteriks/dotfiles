CS_LAST_WORKSPACE: yadm_bootstrap
CS_WORKSPACE_BEGIN: yadm_bootstrap
let SessionLoad = 1
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
exe "cd " . escape(expand("<sfile>:p:h"), ' ')
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +1 yadm_bootstrap_arch_pacman.sh\#\#template
badd +1 yadm_bootstrap_arch_root_post.sh\#\#template
badd +1 yadm_bootstrap_arch_user.sh\#\#template
badd +1 BUFFER\ LIST\ KEY\ REFERENCE
badd +2 ~/.config/yadm/bootstrap\#\#template
argglobal
%argdel
$argadd ~/bin/yadm_bootstrap_arch_pacman.sh\#\#template
edit yadm_bootstrap_arch_pacman.sh\#\#template
edit yadm_bootstrap_arch_root_post.sh\#\#template
edit yadm_bootstrap_arch_user.sh\#\#template
let t:CtrlSpaceLabel = 'yadm bootstrap arch'
set splitbelow splitright
wincmd _ | wincmd |
vsplit
wincmd _ | wincmd |
vsplit
2wincmd h
wincmd w
wincmd w
wincmd t
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 63 + 94) / 189)
exe 'vert 2resize ' . ((&columns * 61 + 94) / 189)
exe 'vert 3resize ' . ((&columns * 63 + 94) / 189)
argglobal
let s:l = 1 - ((0 * winheight(0) + 37) / 74)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
1
normal! 0
wincmd w
argglobal
if bufexists("yadm_bootstrap_arch_root_post.sh\#\#template") | buffer yadm_bootstrap_arch_root_post.sh\#\#template | else | edit yadm_bootstrap_arch_root_post.sh\#\#template | endif
let s:l = 1 - ((0 * winheight(0) + 37) / 74)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
1
normal! 0
wincmd w
argglobal
if bufexists("yadm_bootstrap_arch_user.sh\#\#template") | buffer yadm_bootstrap_arch_user.sh\#\#template | else | edit yadm_bootstrap_arch_user.sh\#\#template | endif
let s:l = 5 - ((0 * winheight(0) + 37) / 74)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
5
normal! 03|
wincmd w
exe 'vert 1resize ' . ((&columns * 63 + 94) / 189)
exe 'vert 2resize ' . ((&columns * 61 + 94) / 189)
exe 'vert 3resize ' . ((&columns * 63 + 94) / 189)
tabedit
edit /home/alteriks/.config/yadm/bootstrap\#\#template
edit ~/.config/yadm/bootstrap\#\#template
set splitbelow splitright
wincmd t
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
argglobal
let s:l = 2 - ((1 * winheight(0) + 37) / 74)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
2
normal! 0
tabnext 1
if exists('s:wipebuf') && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 winminheight=1 winminwidth=1 shortmess=filnxtToOFI
let s:sx = expand("<sfile>:p:r")."x.vim"
if file_readable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &so = s:so_save | let &siso = s:siso_save
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
CS_WORKSPACE_END: yadm_bootstrap
